{
  "name": "@metamask/metamask-eth-abis",
  "version": "3.1.1",
  "description": "The MetaMask Node module for smart contracts ABIs.",
  "homepage": "https://github.com/MetaMask/metamask-eth-abis#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/metamask-eth-abis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/metamask-eth-abis.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "setup": "yarn install && yarn allow-scripts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build:clean && yarn lint && yarn test",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "build:clean": "rimraf dist && yarn build",
    "build": "tsc --project ."
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^1.0.5",
    "@metamask/auto-changelog": "^2.3.0",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^43.0.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^26.4.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false
    }
  }
}
